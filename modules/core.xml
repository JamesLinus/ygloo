<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <!-- External native modules -->
  <project path="external/jpeg-turbo" name="yahoo/ygloo-external-jpeg-turbo" revision="yahoo" />
  <project path="external/webp" name="yahoo/ygloo-external-webp" revision="yahoo" />
  <project path="external/expat" name="yahoo/ygloo-external-expat" revision="yahoo" />
  <project path="external/libpng" name="yahoo/ygloo-external-libpng" revision="yahoo" />
  <project path="external/zlib" name="yahoo/ygloo-external-zlib" revision="yahoo" />
  <project path="external/curl" name="yahoo/ygloo-external-curl" revision="yahoo" />
  <project path="external/axtls" name="yahoo/ygloo-external-axtls" revision="yahoo" />
  <project path="external/openssl" name="yahoo/ygloo-external-openssl" revision="yahoo" />
  <project path="external/jansson" name="yahoo/ygloo-external-jansson" revision="yahoo" />

  <!-- Native SDK -->
  <project path="framework/yosal" name="yahoo/ygloo-yosal" revision="master" />
  <project path="framework/ymagine" name="yahoo/ygloo-ymagine" revision="master" />
  <project path="framework/yperwave" name="yahoo/ygloo-yperwave" revision="master" />

  <!-- Native components, only for building (not required for Build system -->
  <!-- Build system -->
  <project path="build" name="yahoo/ygloo-build" >
    <copyfile src="core/root.mk" dest="Makefile" />
  </project>
</manifest>
